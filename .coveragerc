[run]
branch = True
source =
    agents
    utils
omit =
    tests/*
    */__init__.py
    */__pycache__/*

[report]
show_missing = True
skip_empty = True
precision = 1
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING:
    raise NotImplementedError
    if __name__ == "__main__":

[html]
directory = htmlcov

[xml]
output = coverage.xml
