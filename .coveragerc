[run]
branch = True
source =
    agents
    utils
omit =
    tests/*
    tests/*/*
    scripts/*
    scripts/*/*
    demos/*
    examples/*
    **/__pycache__/*
    */__init__.py
    **/migrations/*
    utils/test_*.py
    test_*.py

[report]
show_missing = True
skip_empty = True
precision = 1
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING:
    raise NotImplementedError
    if __name__ == "__main__":
    def __repr__
    def __str__

[html]
directory = htmlcov

[xml]
output = coverage.xml
